<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="BalancingDMP" />
		<Option pch_mode="2" />
		<Option compiler="avr-gcc" />
		<Build>
			<Target title="Release">
				<Option output="bin/Release/BalancingDMP.elf" prefix_auto="1" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="obj/Release/" />
				<Option type="5" />
				<Option compiler="avr-gcc" />
				<Compiler>
					<Add option="-Os" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="MCU" value="atmega128" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-mmcu=atmega128" />
			<Add option="-Os" />
			<Add option="-DF_CPU=16000000UL" />
			<Add directory="../../Tools/avr8-gnu-toolchain/avr/bin" />
			<Add directory="../../Tools/avr8-gnu-toolchain/avr/include" />
		</Compiler>
		<Linker>
			<Add option="-mmcu=atmega128" />
			<Add option="-Wl,-Map=$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).map,--cref" />
			<Add directory="../../Tools/avr8-gnu-toolchain/avr/lib" />
		</Linker>
		<ExtraCommands>
			<Add after="avr-size --mcu=atmega128 --format=avr $(TARGET_OUTPUT_FILE)" />
			<Add after='cmd /c &quot;avr-objdump -h -S $(TARGET_OUTPUT_FILE) &gt; $(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).lss&quot;' />
			<Add after="avr-objcopy -R .eeprom -R .fuse -R .lock -R .signature -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).hex" />
			<Add after="avr-objcopy --no-change-warnings -j .eeprom --change-section-lma .eeprom=0 -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).eep" />
		</ExtraCommands>
		<Unit filename="../AdRoiT_Files/myADROIT.h" />
		<Unit filename="../AdRoiT_Files/myIMU_DMP.h" />
		<Unit filename="../AdRoiT_Files/mySystem.h" />
		<Unit filename="main.cpp">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="myModule.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
